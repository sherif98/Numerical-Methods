function [exeTime,polynomial,solution,message] = Lagrang(obj, queries)
% implementation goes here
siz = size(obj.points);
message = '';
exeTime = 0;
syms x;
polynomial = symfun(0,x);
if siz(2) ~= 2
    message = 'not valid points';
    return;
end
siz = siz(1);
for i = 1 : 1 : siz
    poly = symfun(1,x);
    for j = 1 : 1 : siz
        if i == j
            continue;
        end
        poly = poly * symfun(x-obj.points(j,1),x);
        poly = poly / symfun
    end
end

end