function [numOfIterations,executionTime,allIteration,aproxRoot,precision,theoError] = Newton(obj,initialGuess)
% implementation goes here
syms x
df = diff(obj.equation, x);
var = obj.equation(initialGuess);
previous = var;
for i=1:1:obj.maxNumOfIterations
    nu
    var = previous - ((obj.equation(previous) / df(previous)));
    if ((abs(var - previous) / var) <= obj.precision)
        aproxRoot = var;
        return;
    end
end
end